{
  "$schema": "http://json.schemastore.org/template",
  "author": "Jason Benfield",
  "classifications": [ "Common", "Library" ],
  "identity": "XTI_Templates.ServiceApp",
  "name": "XTI_Templates.ServiceApp",
  "shortName": "xtiserviceapp",
  "tags": {
    "type": "project",
    "language": "en-US"
  },
  "placeholderFilename": ".keep",
  "symbols": {
    "AppName": {
      "type": "parameter",
      "datatype": "string",
      "isRequired": true,
      "defaultValue": "TheApp",
      "FileRename": "AppName",
      "replaces": "__APPNAME__"
    },
    "RepoName": {
      "type": "parameter",
      "datatype": "string",
      "isRequired": true,
      "defaultValue": "TheRepoName",
      "FileRename": "Domain",
      "replaces": "__REPONAME__"
    }
  }
}